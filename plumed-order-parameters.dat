hhlq4: HISTOGRAM DATA=lq4 GRID_MIN=-1.5 GRID_MAX=1.5 GRID_BIN=200 BANDWIDTH=0.015 STRIDE=1
hhlaq4: HISTOGRAM DATA=laq4 GRID_MIN=-1.5 GRID_MAX=1.5 GRID_BIN=200 BANDWIDTH=0.015 STRIDE=1

DUMPMULTICOLVAR FILE=bulkIh_250K_q4.xyz STRIDE=1 DATA=q4
DUMPMULTICOLVAR FILE=bulkIh_250K_lq4.xyz STRIDE=1 DATA=lq4
DUMPMULTICOLVAR FILE=bulkIh_250K_laq4.xyz STRIDE=1 DATA=laq4

DUMPGRID GRID=hhq4 FILE=bulkIh_250K_histo_q4 STRIDE=1
