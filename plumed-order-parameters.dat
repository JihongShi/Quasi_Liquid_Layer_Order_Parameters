UNITS LENGTH=A
#This is input file for bulkIh-250K.
#This is calculation of Q3s.(3 types)
q3: Q3 SPECIES=1-2880 SWITCH={RATIONAL R_0=3.5 D_MAX=4.0 NN=50 MM=100}
lq3: LOCAL_Q3 SPECIES=q3 SWITCH={RATIONAL R_0=3.5 D_MAX=4.0 NN=50 MM=100}
laq3: LOCAL_AVERAGE SPECIES=q3 SWITCH={RATIONAL R_0=3.5 D_MAX=4.0 NN=50 MM=100}

hhq3: HISTOGRAM DATA=q3 GRID_MIN=-1.5 GRID_MAX=1.5 GRID_BIN=200 BANDWIDTH=0.015 STRIDE=1
hhlq3: HISTOGRAM DATA=lq3 GRID_MIN=-1.5 GRID_MAX=1.5 GRID_BIN=200 BANDWIDTH=0.015 STRIDE=1
hhlaq3: HISTOGRAM DATA=laq3 GRID_MIN=-1.5 GRID_MAX=1.5 GRID_BIN=200 BANDWIDTH=0.015 STRIDE=1

DUMPMULTICOLVAR FILE=bulkIh_250K_q3.xyz STRIDE=1 DATA=q3
DUMPMULTICOLVAR FILE=bulkIh_250K_lq3.xyz STRIDE=1 DATA=lq3
DUMPMULTICOLVAR FILE=bulkIh_250K_laq3.xyz STRIDE=1 DATA=laq3

DUMPGRID GRID=hhq3 FILE=bulkIh_250K_histo_q3 STRIDE=1
DUMPGRID GRID=hhlq3 FILE=bulkIh_250K_histo_lq3 STRIDE=1
DUMPGRID GRID=hhlaq3 FILE=bulkIh_250K_histo_laq3 STRIDE=1




#This is calculation of Q4s.(3 types)
q4: Q4 SPECIES=1-2880 SWITCH={RATIONAL R_0=3.5 D_MAX=4.0 NN=50 MM=100}
